   "source": [
    "import sqlite3\n",
    "\n",
    "conn = sqlite3.connect('uneti_db.db')\n",
    "cursor = conn.cursor()\n",
    "\n",
    "# Đếm tổng số bản ghi trong bảng DiemSinhVien\n",
    "cursor.execute(\"SELECT COUNT(*) FROM DiemSinhVien\")\n",
    "total = cursor.fetchone()[0]\n",
    "\n",
    "print(f\"Tổng số sinh viên có trong danh sách điểm: {total}\")\n",
    "\n",
    "conn.close()"
   ]
  }
