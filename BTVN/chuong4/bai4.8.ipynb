    "import numpy as np \n",
    "import pandas as pd \n",
    "\n",
    "ser = pd.Series(np.random.random(20))\n",
    "\n",
    "labels = [f\"{i}th\" for i in range(1, 11)]\n",
    "ser_deciles = pd.qcut(ser, q=10, labels=labels)\n",
    "\n",
    "print(ser_deciles)"
